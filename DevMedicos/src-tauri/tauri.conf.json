{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "devmedicos",
  "version": "0.1.0",
  "identifier": "com.devmedicos.app",
  "build": {
  "beforeDevCommand": "npm run dev",
  "devUrl": "http://localhost:5173",
  "beforeBuildCommand": "npm run build",
  "frontendDist": "../dist"
}
,
  "app": {
    "windows": [
      {
        "title": "devmedicos",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
        "icons/logo.png",
        "icons/logo.ico"
    ]
  }
}
